<local:FrmODBase x:Class="OpenDental.FrmChildParents"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="425" Height="563" 
             Text="Parent Select" MinMaxBoxes="False">
	<Grid Background="#FFFCFDFE" >
		<ui:Grid x:Name="gridMain" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="12,10,100,10" Title="Parents" />
		<ui:Button x:Name="butAdd" HorizontalAlignment="Right" Width="75" Margin="0,10,10,0" Text="_Add" Click="butAdd_Click" RenderTransformOrigin="0.543,1.257" Icon="Add" Height="24" />
		<ui:Button x:Name="butOK" Margin="0,0,10,10" Text="OK" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Click="butOK_Click" />
	</Grid>
</local:FrmODBase>